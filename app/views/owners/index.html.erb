<p id="notice"><%= notice %></p>

<h1>Owners</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Age</th>
      <th>Sex</th>
      <th>Income</th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td><%= owner.id %></td>
        <td><%= owner.name %></td>
        <td><%= owner.age %></td>
        <td><%= owner.sex %></td>
        <td><%= owner.income %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h1>スコープをかけたownersテーブル</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Age</th>
    </tr>
  </thead>

  <tbody>
    <% @owner_scope.each do |owner_scope| %>
      <tr>
        <td><%= owner_scope.id %></td>
        <td><%= owner_scope.name %></td>
        <td><%= owner_scope.age %></td>
      </tr>
    <% end %>
  </tbody>
</table>
