<p id="notice"><%= notice %></p>

<h1>ownersテーブル</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Age</th>
      <th>Sex</th>
      <th>Income</th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td><%= owner.id %></td>
        <td><%= owner.name %></td>
        <td><%= owner.age %></td>
        <td><%= owner.sex %></td>
        <td><%= owner.income %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h1>catsテーブル</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Owner_ID</th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
      <% owner.cats.each do |cat|%>
        <tr>
          <td><%= owner.id %></td>
          <td><%= cat.name %></td>
          <td><%= cat.owner_id %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to "Catページへ", cats_url %>
