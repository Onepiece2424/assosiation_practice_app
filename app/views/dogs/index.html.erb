<h1>dogsテーブル</h1>
<table class="teble">
  <thead>
    <tr>
      <th class="header1">id</th>
      <th class="header2">name</th>
      <th class="header3">owner_id</th>
    </tr>
  </thead>
  <tbody>
    <% @dogs.each do |dog|  %>
    <tr>
      <td><%= dog.id %></td>
      <td><%= dog.name %></td>
      <td><%= dog.owner_id %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to "Catページへ", cats_path %>
